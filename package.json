{
  "name": "tree-sitter-validatetest",
  "version": "0.1.0",
  "description": "GStreamer ValidateTest grammar for tree-sitter",
  "repository": {
    "type": "git",
    "url": "https://github.com/gstreamer/tree-sitter-validatetest"
  },
  "license": "MIT",
  "author": "Thibault Saunier",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "gstreamer",
    "validate",
    "validatetest",
    "tree-sitter",
    "parser"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.0",
    "prebuildify": "^6.0.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "build-wasm": "tree-sitter build --wasm",
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "tree-sitter test",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.validatetest",
      "injection-regex": "^validatetest$",
      "file-types": [
        "validatetest"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ]
}